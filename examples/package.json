{
  "name": "@bluehive/random-examples",
  "version": "1.0.0",
  "description": "Interactive examples for @bluehive/random library",
  "private": true,
  "scripts": {
    "serve": "npx serve .",
    "start": "npm run check-browser-js && npx serve . -l 3000",
    "check-browser-js": "node -e \"if (!require('fs').existsSync('./browser.js')) { console.error('\\nError: browser.js not found. Please run \\'npm run examples\\' from the root directory.\\n'); process.exit(1); }\"",
    "node-example": "node node-example.js",
    "typescript-example": "npx tsc typescript-example.ts --target es2020 --module commonjs && node typescript-example.js",
    "dev": "npx serve . -l 3000 --cors",
    "build-ts": "npx tsc typescript-example.ts --target es2020 --module commonjs",
    "clean": "rm -f *.js.map typescript-example.js browser.js browser.js.map"
  },
  "keywords": [
    "random",
    "examples",
    "crypto",
    "secure",
    "demo"
  ],
  "author": "BlueHive Health",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.2.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bluehive/random": "^1.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
