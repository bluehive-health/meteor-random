{
  "name": "@bluehive/random",
  "version": "1.0.2",
  "description": "Random number generator and utilities with cryptographically strong PRNGs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "browser": "./dist/browser.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:legacy": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "examples": "npm run build && npm run examples:copy && cd examples && npm install && npm run start",
    "examples:copy": "cpx \"dist/browser.js*\" examples/",
    "examples:dev": "npm run build && npm run examples:copy && cd examples && npm install && npm run dev",
    "examples:build": "npm run build && npm run examples:copy",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --ignore-pattern src/**/__tests__/**",
    "lint:fix": "eslint src/**/*.ts --ignore-pattern src/**/__tests__/** --fix",
    "lint:all": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm test",
    "dev": "rollup -c --watch"
  },
  "keywords": [
    "random",
    "prng",
    "crypto",
    "secure",
    "alea",
    "generator"
  ],
  "author": "BlueHive Health",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluehive-health/meteor-random.git"
  },
  "homepage": "https://bluehive-health.github.io/meteor-random/",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cpx2": "^8.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "rimraf": "^6.0.1",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
